                <RelativeLayout
                    android:id="@+id/yesmedicinebtn"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginStart="@dimen/fourty"
                    android:layout_marginTop="@dimen/thirty"
