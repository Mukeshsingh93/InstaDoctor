<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <import type="android.view.View"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingStart="@dimen/twoenty"
        android:paddingEnd="@dimen/twoenty">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/backbtn"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/backbtn_back"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/thirty"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/customersupporttxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/customersupport"
            android:fontFamily="@font/poppin_reg"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="@dimen/textsize_etn"
            app:layout_constraintTop_toBottomOf="@+id/backbtn"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/twoenty"
            />




                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/customersupportimg"
                    android:layout_width="wrap_content"
                    android:layout_height="260dp"
                    android:padding="@dimen/thirty"
                    android:src="@drawable/customersupport"
                    app:layout_constraintTop_toBottomOf="@+id/customersupporttxt"
                    app:layout_constraintStart_toStartOf="parent"
                    />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/helpquickly"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:visibility="gone"
                    android:background="@drawable/customersprt_drawable"
                    app:layout_constraintTop_toBottomOf="@+id/customersupportimg"
                    tools:layout_editor_absoluteX="20dp">


                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="@dimen/twoenty"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:src="@drawable/helpicon"
                        android:scaleType="fitXY"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/poppin_reg"
                            android:text="@string/gethelpquickly"
                            android:textColor="@color/blue"
                            android:textSize="@dimen/textsize_sixteen"
                            android:textStyle="bold" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/poppin_reg"
                            android:text="@string/browseourfaqs"
                            android:textColor="@color/black"
                            android:textSize="@dimen/textsize_fourteen" />


                    </LinearLayout>

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/rightarrow_blue"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginEnd="@dimen/five"/>

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/livechatsupport"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:background="@drawable/customersprt_drawable"
                    app:layout_constraintTop_toBottomOf="@+id/customersupportimg"
                    android:layout_marginStart="@dimen/ten"
                    android:layout_marginEnd="@dimen/ten">


                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="@dimen/twoenty"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:src="@drawable/chatsupportimg"
                        />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/poppin_reg"
                            android:text="@string/livechartsupport"
                            android:textColor="@color/lightblue"
                            app:fontFamily="@font/poppin_semi_bold"
                            android:textSize="@dimen/textsize_sixteen"
                            android:textStyle="bold" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/poppin_reg"
                            android:text="@string/sendamessage"
                            app:fontFamily="@font/poppin_reg"
                            android:textColor="@color/black"
                            android:textSize="@dimen/textsize_fourteen" />


                    </LinearLayout>

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/chat"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/rightarrow_lightblue"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginEnd="@dimen/five"/>

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:background="@drawable/customersprt_drawable"
                    app:layout_constraintTop_toBottomOf="@+id/livechatsupport"
                    android:layout_marginTop="@dimen/twoenty"
                    android:layout_marginStart="@dimen/ten"
                    android:layout_marginEnd="@dimen/ten">

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="@dimen/twoenty"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:src="@drawable/callsupportimg"
                        />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/poppin_reg"
                            android:text="@string/callsupporttwofourseven"
                            android:textColor="@color/lightblue"
                            app:fontFamily="@font/poppin_semi_bold"
                            android:textSize="@dimen/textsize_sixteen"
                            android:textStyle="bold" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/poppin_reg"
                            android:text="+971 508800842"
                            app:fontFamily="@font/poppin_reg"
                            android:textColor="@color/black"
                            android:textSize="@dimen/textsize_fourteen" />


                    </LinearLayout>

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/phonecalling"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/rightarrow_lightblue"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginEnd="@dimen/five"/>

                </androidx.constraintlayout.widget.ConstraintLayout>










</androidx.constraintlayout.widget.ConstraintLayout>
</layout>
